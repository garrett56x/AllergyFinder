<head>
<!-- Bootstrap CDN -->
	<script src="//ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
	<link href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css" rel="stylesheet">
	<script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js"></script>
	<link href='//fonts.googleapis.com/css?family=Quicksand' rel='stylesheet' type='text/css'>
	<!-- High Charts CDN -->
	<script src="//code.highcharts.com/highcharts.js"></script>
	<script src="//code.highcharts.com/modules/exporting.js"></script>
	<!-- CSS -->
	<style>
	.btn{
		margin-top:10px;
		display:inline-block;
	}
	</style>
	<!-- High Charts JQuery -->
	<script>
	$( document ).ready(function() {
		$(function () {
		    $('#container').highcharts({
		        chart: {
		            type: 'spline'
		        },
		        title: {
		            text: 'Time of Day Analysis'
		        },
		        subtitle: {
		            text: 'Analysis of reactions in 24hrs'
		        },
		        xAxis: {
		            categories: ['6:00 AM', '7:00 AM', '8:00 AM', '9:00 AM', '10:00 AM', '11:00 AM', '12:00 PM', 
		            			'1:00 PM', '2:00 PM', '3:00 PM', '4:00 PM', '5:00 PM', '6:00 PM', '7:00 PM', '8:00 PM', '9:00 PM', '10:00 PM', '11:00 PM', '12:00 PM', '1:00 AM', '2:00 AM', '3:00 AM', '4:00 AM', '5:00 AM']
		        },
		        yAxis: {
		            title: {
		                text: 'Severity'
		            },
		            labels: {
		                formatter: function () {
		                    return this.value;
		                }
		            }
		        },
		        tooltip: {
		            crosshairs: true,
		            shared: true
		        },
		        plotOptions: {
		            spline: {
		                marker: {
		                    radius: 4,
		                    lineColor: '#666666',
		                    lineWidth: 1
		                }
		            }
		        },
		        series: [{
		            name: 'Reactions',
		            marker: {
		                symbol: 'square'
		            },
		            data: [1, 0, 0, 0, 0, 0, 0, {
		                y: 9,
		                marker: {
		                    // symbol: 'url(http://i.imgur.com/r4cifKt.png)'
		                }
		            }, 7, 6, 5, 2, 0, 2]

		        }, {
		            name: 'Meals',
		            marker: {
		                symbol: 'diamond'
		            },
		            data: [{
		                y: 0,
		                marker: {
		                    // symbol: 'url(http://www.highcharts.com/demo/gfx/sun.png)'
		                }
		            }, 0, 0, 7, 0, 0, 9, 0, 8, 0, 0, 0, 5]
		        }]
		    });
		});
	});
	</script>
		<!-- Navbar Render -->
		<%= render 'partials/navbar' %>
</head>
<body>
	
<div class='container'>
	<div class='row'>
		<div class='text-center'>
			<div id="container" style="min-width: 310px; height: 400px; margin: 0 auto"></div>
		</div>
	</div>
	<div class='row text-center'>
	<!-- Navigation Buttons -->
		<a href="/mains/show"><button class='btn btn-info btn-xs center-block'>Category Distribution</button></a>
		<button class='btn btn-info btn-xs center-block' disabled>Time of Day Analyis</button>
	</div>
</div>
</body>
